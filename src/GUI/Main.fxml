                                 <font>
                                    <Font name="Avenir Book" size="14.0" />
                                 </font></Button>
                              <Button fx:id="PasswordAttacks_ServerButton" layoutX="47.0" layoutY="172.0" mnemonicParsing="false" onMouseClicked="#PasswordAttacks_StartServer" prefHeight="36.0" prefWidth="200.0" style="-fx-background-color: #dfab40; -fx-background-radius: 0;" text="Start Server" textFill="#333946">
                                 <font>
                                    <Font name="Avenir Book" size="14.0" />
                                 </font></Button>
                              <ComboBox fx:id="PasswordAttacks_AttackMode" layoutX="649.0" layoutY="172.0" onKeyPressed="#PasswordAttacks_AttackModeChanged" prefHeight="36.0" prefWidth="200.0" />
                              <TextField fx:id="PasswordAttacks_ServerPort" layoutX="47.0" layoutY="73.0" prefHeight="40.0" prefWidth="200.0" promptText="Server port number" style="-fx-background-radius: 0; -fx-background-insets: 0;">
                                 <font>
                                    <Font name="Avenir Book" size="14.0" />
                                 </font></TextField>
                              <TextField fx:id="PasswordAttacks_ClientTargePort" layoutX="649.0" layoutY="123.0" prefHeight="40.0" prefWidth="200.0" promptText="Target port" style="-fx-background-radius: 0; -fx-background-insets: 0;">
                                 <font>
                                    <Font name="Avenir Book" size="14.0" />
                                 </font></TextField>
                              <Slider fx:id="PasswordAttacks_BruteForcePasswordLength" blockIncrement="1.0" layoutX="347.0" layoutY="115.0" majorTickUnit="1.0" max="6.0" min="1.0" minorTickCount="0" onInputMethodTextChanged="#PasswordAttacks_BruteForceConfigChange" prefWidth="200.0" showTickLabels="true" showTickMarks="true" snapToTicks="true" style="-fx-text-fill: #dfab40;" />
                              <ProgressBar fx:id="Traversed" layoutX="645.0" layoutY="268.0" prefHeight="30.0" prefWidth="207.0" progress="0.0" />
                              <Label alignment="CENTER" layoutX="374.0" layoutY="67.0" prefHeight="26.0" prefWidth="142.0" text="Brute Force Attack" textFill="#dfab40">
                                 <font>
                                    <Font name="Avenir Book" size="14.0" />
                                 </font>
                              </Label>
                              <Label alignment="CENTER" layoutX="248.0" layoutY="34.0" prefHeight="27.0" prefWidth="401.0" text="Attack Setup" textFill="WHITE">
                                 <font>
                                    <Font name="Avenir Book" size="18.0" />
                                 </font>
                              </Label>
                              <Label alignment="CENTER" layoutX="374.0" layoutY="92.0" prefHeight="26.0" prefWidth="142.0" text="Password Length" textFill="#dfab40">
                                 <font>
                                    <Font name="Avenir Book" size="14.0" />
                                 </font>
                              </Label>
                           </children>
                        </AnchorPane>
                    </content>
                  </Tab>
                  <Tab text="Stream Cipher + Steganography">
