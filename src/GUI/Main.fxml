                              <TextArea fx:id="PasswordAttacks_ClientOutput" editable="false" layoutX="462.0" layoutY="333.0" prefHeight="133.0" prefWidth="391.0" promptText="Attempt Logs" style="-fx-background-radius: 0;" wrapText="true">
